<view class="top font14 color1 bg1">
	黑龙江
</view>
<view class="year color2 font13 bg1">
	<view class="year-item {{web_year==2018?'item-acive':''}}" bindtap="changeYear" data-year='2018'>2018年</view>
	<view class="year-item {{web_year==2017?'item-acive':''}}" bindtap="changeYear" data-year='2017'>2017年</view>
	<view class="year-item {{web_year==2016?'item-acive':''}}" bindtap="changeYear" data-year='2016'>2016年</view>
	<view class="year-item {{web_year==2015?'item-acive':''}}" bindtap="changeYear" data-year='2015'>2015年</view>
	<view class="year-item {{web_year==2014?'item-acive':''}}" bindtap="changeYear" data-year='2014'>2014年</view>
</view>
<view class="box">
	<view class="tabe color2 font13">
		<view class="th color1">
			<view class="td">批次</view>
			<view class="td">文科</view>
			<view class="td">理科</view>
		</view>
		<view class="tr">
			<view class="td">本科一批</view>
			<view class="td">{{web_data[web_year]['文科']['本科一批']}}</view>
			<view class="td">{{web_data[web_year]['理科']['本科一批']}}</view>
		</view>
		<view class="tr">
			<view class="td">本科二批</view>
			<view class="td">{{web_data[web_year]['文科']['本科二批']}}</view>
			<view class="td">{{web_data[web_year]['理科']['本科二批']}}</view>
		</view>
		<view class="tr">
			<view class="td">本科三批</view>
			<view class="td">{{web_data[web_year]['文科']['本科三批']?web_data[web_year]['文科']['本科三批']:'-'}}</view>
			<view class="td">{{web_data[web_year]['理科']['本科三批']?web_data[web_year]['理科']['本科三批']:'-'}}</view>
		</view>
		<view class="tr">
			<view class="td">专科批</view>
			<view class="td">{{web_data[web_year]['文科']['专科批']?web_data[web_year]['文科']['专科批']:'-'}}</view>
			<view class="td">{{web_data[web_year]['理科']['专科批']?web_data[web_year]['理科']['专科批']:'-'}}</view>
		</view>
		<view class="tr">
			<view class="td">艺术类本科</view>
			<view class="td">-</view>
			<view class="td">-</view>
		</view>
		<view class="tr">
			<view class="td">体育类本科</view>
			<view class="td">-</view>
			<view class="td">-</view>
		</view>
	</view>
</view>
<view class="color3 font11" style="padding:30rpx;">注：默认抓取的是定位城市</view>
 
