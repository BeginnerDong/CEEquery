	background: #F5F5F5;
	width: 100%;
	box-sizing: border-box;
	padding: 4%;
}
.topRight{
	width: 100%;
	height:60rpx;
	position: relative;
	z-index: 10;
	box-shadow: 0rpx 2rpx 10rpx #D1D1D1;
}

.topRight image{
	margin-left: 22rpx;
	margin-bottom: 8rpx;
}
.topRight input{
	width: 56%;
	margin-left: 30rpx;
	position: relative; 
	top: 6rpx;
}
.search-box{
	border-radius: 0rpx 20rpx 20rpx 0rpx;
	height: 60rpx;
	width: 18%;
	line-height: 60rpx;
	position: absolute;
	bottom: 0rpx;
	right: 0rpx;
}

.he10{
	width: 100%;
	height: 10rpx;
	background: #F5F5F5;
}
.list-two{
	box-sizing: border-box;
	padding: 30rpx 4%;
}
.list-left{
	width: 22%;
	height: 152rpx;
	border-radius: 50%;
	overflow: hidden;
}
.list-left image{
	width: 100%;
	height: 100%;
}
.one-center{
	width: 74%;
	margin-left: 20rpx;
	position: relative;
	top: -8rpx;
	margin-left: 20rpx;
}
.one-color{
	margin: 30rpx 0rpx;
	line-height:36rpx ;
}
.one-color view{
	margin-right: 20rpx;
	background: #F4F7FC;
	padding: 2rpx 16rpx;
	color:rgb(104,165,255);
}


.fixd{
	position: fixed;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.6);
	top: 0rpx;
	z-index: 1000;
}
.fixd-box{
	width:90%;
	height: 400rpx;
	margin: 50% 5%;
	box-sizing: border-box;
	padding: 30rpx;
}
.fixd-box button{
	display: inline-block;
	width: 40%;
	height: 70rpx;
	line-height:70rpx;
	margin: 80rpx 30%;
}