@import "../../wxParse/wxParse.wxss"
.top{
	width: 100%;
	height: 260rpx;
	text-align: center;
	position: position;
}
.logo{
	 width: 180rpx;
	 height: 180rpx;
	 position: absolute;
	 top: 70rpx;
	 left: 39%;
}
.one-color{
	margin: 30rpx 0rpx;
	line-height:36rpx ;
}
.one-color view{
	margin-right: 20rpx;
	background: #F4F7FC;
	padding: 2rpx 16rpx;
	color:rgb(104,165,255);
}
.he10{
	width: 100%;
	height: 10rpx;
	background: #F5F5F5;
}
.line{
	box-sizing: border-box;
	padding: 40rpx 4% 0rpx;
}
.line-red{
	border-left: solid 6rpx #2E82FF;
	text-indent: 20rpx;
}
.text{
	box-sizing: border-box;
	padding: 30rpx;
	line-height: 38rpx;
	height: 230rpx;
}
.open-list{
	border: solid 1rpx #2E82FF;
	
	height: 46rpx;
	line-height: 46rpx;
	text-align: center;
	display: inline-block;
	border-radius: 8rpx;
	margin: 20rpx 2.6% 0rpx;
	padding: 0 10rpx;
}

.left{
	width: 100rpx;
	height: 250rpx;
	text-align: center;
	position: fixed;
	right: 0rpx;
	top: 20%
}
.left image{
	width: 60rpx;
	height: 60rpx;
}
.contrast-box{
	border: solid 1rpx #E5E5E5;
	width: 220rpx;
	height: 300rpx;
	position: fixed;
	right: 0rpx;
	top: 64%;
}
.contrast-top{
	width: 100%;
	height: 56rpx;
	border-bottom: solid 1rpx #E5E5E5;
	background: #F0F0F0;
	text-align: center;
	line-height: 56rpx;
}
.contrast{
	width: 70%;
	height: 42rpx;
	border: solid 1rpx #E5E5E5;
	margin:30rpx 15% 0rpx;
	text-align: center;
	line-height: 42rpx;
}



.btm-nav{
	width: 100%;
	height: 80rpx;
	position: fixed;
	bottom: 0rpx;
}
.nav_item{
	width: 33.3%;
	height: 100%;
	text-align: center;
	line-height: 80rpx;
	color: #fff;
	font-size: 28rpx;
	display: inline-block;
}