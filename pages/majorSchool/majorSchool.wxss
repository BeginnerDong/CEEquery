.fixd{
	position: fixed;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.6);
	top: 0rpx;
	z-index: 1000;
}
.fixd-box{
	width:90%;
	height:80%;
	box-sizing:border-box;
	padding:30rpx;
	position:absolute;
	top:10%;
	left:5%;
}
.fixd-box button{
	display: inline-block;
	width: 40%;
	height: 70rpx;
	line-height:70rpx;
	margin: 80rpx 30%;
}
.vip-img{
	width:100%;
	display: flex;
	justify-content: center;
}
.vip-img image{
	width:70%
}
.contrast-box{
	border: solid 1rpx #E5E5E5;
	width: 220rpx;
	height: 300rpx;
	position: fixed;
	right: 0rpx;
	top: 64%;
}
.contrast-top{
	width: 100%;
	height: 56rpx;
	border-bottom: solid 1rpx #E5E5E5;
	background: #F0F0F0;
	text-align: center;
	line-height: 56rpx;
}
.contrast{
	width: 70%;
	height: 42rpx;
	border: solid 1rpx #E5E5E5;
	margin:30rpx 15% 0rpx;
	text-align: center;
	line-height: 42rpx;
}



.btm-nav{
